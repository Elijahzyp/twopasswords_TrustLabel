# 1. Enter your email credentials
# 2. Rename this file to 'config.yaml'
---
- files:        # Local file paths
    db_path:    "config/accounts.sqlite"
    user_image: "config/user_face.jpg"
    last_image: "config/last_face.jpg"
    words:      "config/words.txt"

- email:        # Email settings
    address:    "replace-me@example.org"
    password:   "replace-me"
    server:     "smtp.gmail.com"
    port:       "465"

- logging:     # Logging settings
    logging:    Flase
    log_path:   /tmp/